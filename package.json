{
  "name": "ownercommunity-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@azure/msal-browser": "^2.22.1",
    "@craco/craco": "^7.0.0-alpha.3",
    "@craftjs/core": "^0.2.0-beta.5",
    "@headlessui/react": "^1.4.3",
    "@nosferatu500/react-sortable-tree": "^4.0.6",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "antd-img-crop": "^4.2.0",
    "axios": "^0.26.1",
    "babel-plugin-relay": "^15.0.0",
    "browser-image-compression": "^1.0.17",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.11.0",
    "graphql": "^16.3.0",
    "lodash": "^4.17.21",
    "rc-resize-observer": "^1.2.0",
    "react": "^17.0.2",
    "react-contenteditable": "^3.3.6",
    "react-dom": "^17.0.2",
    "react-relay": "^15.0.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.5",
    "uniqid": "^5.4.0",
    "use-local-storage-state": "^13.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run relay && craco start",
    "build": "npm run relay && craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "relay": "relay-compiler"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.18",
    "@types/lodash": "^4.14.182",
    "@types/react-relay": "^14.1.3",
    "@types/relay-runtime": "^14.1.10",
    "@types/uniqid": "^5.3.2",
    "autoprefixer": "^10.4.2",
    "babel-plugin-macros": "^3.1.0",
    "craco-less": "^2.1.0-alpha.0",
    "postcss": "^8.4.6",
    "relay-compiler": "^15.0.0",
    "storybook-addon-customize-antd-theme": "^1.1.0",
    "tailwindcss": "^3.0.18",
    "webpack": "^5.68.0"
  },
  "relay": {
    "src": "./src",
    "language": "typescript",
    "schema": "./schema.graphql",
    "schemaExtensions": [
      "../data-access/graphql/schema/core/",
      "../data-access/graphql/schema/interfaces/",
      "../data-access/graphql/schema/shared/",
      "../data-access/graphql/schema/types/"
    ],
    "customScalars": {
      "ObjectID": "string"
    },
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  }
}
